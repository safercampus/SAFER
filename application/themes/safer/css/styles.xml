  <set name="Site Styles">
    <style name="Safer Logo Image" variable="safer-logo" type="image" />
    <style name="Safer Logo Height Size" variable="safer-logo-height" type="size" />
    <style name="Safer Logo Width Size" variable="safer-logo-width" type="size" />
    <style name="Safer Logo Margin Top Size" variable="safer-logo-margin-top" type="size" />
    <style name="Button Color" variable="button" type="color" />
